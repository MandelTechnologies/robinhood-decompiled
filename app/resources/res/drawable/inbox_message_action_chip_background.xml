<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/colorRipple">
    <item>
        <shape>
            <corners android:radius="1000dp"/>
            <stroke
                android:width="1dp"
                android:color="?android:attr/textColorPrimary"/>
