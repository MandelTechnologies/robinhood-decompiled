<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:height="6dp"
                android:width="6dp"/>
        </shape>
    </item>
    <item
        android:gravity="start|top"
        android:left="0dp"
        android:top="0dp"
        android:right="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
            <size
                android:height="3dp"
                android:width="3dp"/>
        </shape>
    </item>
    <item
        android:gravity="end|bottom"
        android:left="3dp"
        android:top="3dp"
        android:right="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
            <size
                android:height="3dp"
                android:width="3dp"/>
        </shape>
    </item>
</layer-list>
