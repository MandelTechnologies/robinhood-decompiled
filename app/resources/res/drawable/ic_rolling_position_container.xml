<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?attr/colorRipple">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/rolling_position_container_corner"/>
            <solid android:color="#fff"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/rolling_position_container_corner"/>
            <solid android:color="@color/transparent"/>
            <stroke
                android:width="1dp"
                android:color="#fff"/>
        </shape>
    </item>
</ripple>
