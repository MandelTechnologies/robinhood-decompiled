<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.withpersona.sdk2.inquiry.shared.p422ui.Pi2NavigationBar
            android:id="@+id/navigation_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:importantForAccessibility="yes"
            android:accessibilityTraversalAfter="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/governmentid_header_image_container"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_governmentid_instructions_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/navigation_bar"/>
        <ImageView
            android:id="@+id/imageview_governmentid_header_image"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:scaleType="fitStart"
            android:importantForAccessibility="no"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_governmentid_instructions_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/navigation_bar"/>
        <androidx.constraintlayout.widget.Barrier
            android:id="@+id/governmentid_header_bottom_barrier"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:barrierDirection="bottom"
            app:constraint_referenced_ids="governmentid_header_image_container,imageview_governmentid_header_image"/>
        <TextView
            android:id="@+id/textview_governmentid_instructions_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_governmentid_instructions_body"
            app:layout_constraintEnd_toEndOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintStart_toStartOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintTop_toBottomOf="@+id/governmentid_header_bottom_barrier"
            app:layout_constraintVertical_bias="0"
            app:layout_constraintVertical_chainStyle="packed"
            app:layout_constraintWidth_max="500dp"
            style="?attr/textAppearanceHeadline6"/>
        <TextView
            android:id="@+id/textview_governmentid_instructions_body"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_governmentid_instructionslistheader"
            app:layout_constraintEnd_toEndOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintStart_toStartOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintTop_toBottomOf="@+id/textview_governmentid_instructions_title"
            app:layout_constraintWidth_max="500dp"
            style="?attr/textAppearanceSubtitle1"/>
        <TextView
            android:textStyle="bold"
            android:id="@+id/textview_governmentid_instructionslistheader"
            android:paddingTop="24dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintEnd_toEndOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintTop_toBottomOf="@+id/textview_governmentid_instructions_body"
            app:layout_constraintWidth_max="500dp"/>
        <View
            android:id="@+id/list_divider"
            android:background="?android:attr/listDivider"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="24dp"
            android:importantForAccessibility="no"
            android:layout_marginHorizontal="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textview_governmentid_instructionslistheader"
            app:layout_constraintWidth_max="500dp"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview_governmentid_idlist"
            android:paddingBottom="16dp"
            android:clipToPadding="false"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintBottom_toTopOf="@+id/textview_governmentid_instructions_disclaimer"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/list_divider"
            app:layout_constraintWidth_max="500dp"/>
        <TextView
            android:gravity="center"
            android:id="@+id/textview_governmentid_instructions_disclaimer"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintStart_toStartOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintTop_toBottomOf="@+id/recyclerview_governmentid_idlist"
            app:layout_constraintWidth_max="500dp"
            style="?attr/textAppearanceCaption"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
