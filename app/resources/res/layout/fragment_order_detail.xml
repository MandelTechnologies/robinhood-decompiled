<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/order_detail_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/legacy_container"
        android:visibility="gone"
        layout="@layout/include_order_detail_view"/>
    <include
        android:id="@+id/server_driven_container"
        android:visibility="gone"
        layout="@layout/include_order_detail_view_server_driven"/>
    <com.robinhood.android.common.p088ui.ShimmerLoadingView
        android:id="@+id/loading_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</FrameLayout>
