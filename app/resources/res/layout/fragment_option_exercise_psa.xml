<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceDisplayMedium"
        android:id="@+id/exercise_psa_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:text="@string/exercise_psa_title"
        android:layout_marginStart="@dimen/rds_spacing_medium"
        android:layout_marginEnd="@dimen/rds_spacing_medium"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:id="@+id/exercise_psa_description"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_small"
        android:layout_marginBottom="@dimen/rds_spacing_xsmall"
        android:text="@string/exercise_psa_description"
        android:layout_marginStart="@dimen/rds_spacing_medium"
        android:layout_marginEnd="@dimen/rds_spacing_medium"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_title"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_first_icon"
        android:text="@string/general_label_one"
        app:layout_constraintBottom_toBottomOf="@+id/exercise_psa_first_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_first_title"
        style="@style/NumberedIcon"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_first_title"
        android:layout_marginTop="@dimen/exercise_psa_list_top_margin"
        android:text="@string/exercise_psa_otm_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_first_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_description"
        style="@style/ExercisePSARow.Title"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_first_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_first_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_first_title"
        style="@style/ExercisePSARow.Description"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_second_icon"
        android:text="@string/general_label_two"
        app:layout_constraintBottom_toBottomOf="@+id/exercise_psa_second_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_second_title"
        style="@style/NumberedIcon"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_second_title"
        android:layout_marginTop="@dimen/rds_spacing_medium"
        android:text="@string/exercise_psa_time_value_label"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_second_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_first_description"
        style="@style/ExercisePSARow.Title"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_second_description"
        android:text="@string/exercise_psa_time_value_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_second_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_second_title"
        style="@style/ExercisePSARow.Description"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_third_icon"
        android:text="@string/general_label_three"
        app:layout_constraintBottom_toBottomOf="@+id/exercise_psa_third_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_third_title"
        style="@style/NumberedIcon"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_third_title"
        android:layout_marginTop="@dimen/rds_spacing_medium"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_third_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_second_description"
        style="@style/ExercisePSARow.Title"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/exercise_psa_third_description"
        android:layout_marginBottom="@dimen/rds_spacing_default"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/exercise_psa_third_icon"
        app:layout_constraintTop_toBottomOf="@+id/exercise_psa_third_title"
        style="@style/ExercisePSARow.Description"/>
</androidx.constraintlayout.widget.ConstraintLayout>
