<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.robinhood.shared.common.agreementview.RdsAgreementView
        android:id="@+id/agreement_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:acceptButtonText="@string/crypto_upgrade_accept"
        app:descriptionText="@string/crypto_upgrade_submit_disclaimer"
        app:reviewButtonText="@string/crypto_upgrade_review"/>
    <com.robinhood.android.common.p088ui.ShimmerLoadingView
        android:id="@+id/loading_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</FrameLayout>
