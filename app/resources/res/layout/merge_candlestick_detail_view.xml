<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/open_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_open"
        app:layout_constraintBottom_toTopOf="@+id/high_label_txt"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread_inside"
        style="@style/CandlestickDetailViewTextView.Label"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/high_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_high"
        app:layout_constraintBottom_toTopOf="@+id/volume_label_txt"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toBottomOf="@+id/open_label_txt"
        style="@style/CandlestickDetailViewTextView.Label"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/volume_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_volume"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/left_guideline"
        app:layout_constraintTop_toBottomOf="@+id/high_label_txt"
        style="@style/CandlestickDetailViewTextView.Label"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/open_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/open_label_txt"
        app:layout_constraintEnd_toStartOf="@+id/center_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/open_label_txt"
        style="@style/CandlestickDetailViewTextView.Value.Left"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/high_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/high_label_txt"
        app:layout_constraintEnd_toStartOf="@+id/center_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/high_label_txt"
        style="@style/CandlestickDetailViewTextView.Value.Left"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/volume_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/volume_label_txt"
        app:layout_constraintEnd_toStartOf="@+id/center_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/volume_label_txt"
        style="@style/CandlestickDetailViewTextView.Value.Left"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/close_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_close"
        app:layout_constraintBaseline_toBaselineOf="@+id/open_value_txt"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="@+id/center_guideline"
        style="@style/CandlestickDetailViewTextView.Label.Right"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/low_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_low"
        app:layout_constraintBaseline_toBaselineOf="@+id/high_value_txt"
        app:layout_constraintStart_toStartOf="@+id/center_guideline"
        style="@style/CandlestickDetailViewTextView.Label.Right"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/percent_change_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/candlestick_percent_change"
        app:layout_constraintBaseline_toBaselineOf="@+id/volume_value_txt"
        app:layout_constraintStart_toStartOf="@+id/center_guideline"
        style="@style/CandlestickDetailViewTextView.Label.Right"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/close_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/close_label_txt"
        app:layout_constraintEnd_toEndOf="@+id/right_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/close_label_txt"
        style="@style/CandlestickDetailViewTextView.Value"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/low_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/low_label_txt"
        app:layout_constraintEnd_toEndOf="@+id/right_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/low_label_txt"
        style="@style/CandlestickDetailViewTextView.Value"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textColor="?attr/colorPrimary"
        android:id="@+id/percent_change_value_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBaseline_toBaselineOf="@+id/percent_change_label_txt"
        app:layout_constraintEnd_toStartOf="@+id/right_guideline"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintStart_toEndOf="@+id/percent_change_label_txt"
        app:scarlet_childContext="neverAnimated"
        style="@style/CandlestickDetailViewTextView.Value"/>
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/center_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/left_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.1"/>
    <androidx.constraintlayout.widget.Guideline
        android:orientation="vertical"
        android:id="@+id/right_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.9"/>
</merge>
