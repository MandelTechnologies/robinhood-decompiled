<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/row_container_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/row_content_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent">
            <ViewStub
                android:id="@+id/row_leading_icon"
                android:layout="@layout/include_rds_row_leading_icon"
                android:inflatedId="@+id/row_leading_icon"
                android:layout_marginEnd="@dimen/rds_spacing_default"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_start_barrier"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="?attr/rowLeadingIconStyle"/>
            <ViewStub
                android:id="@+id/row_number_pog"
                android:layout="@layout/include_rds_number_pog"
                android:inflatedId="@+id/row_number_pog"
                android:layout_marginEnd="@dimen/rds_spacing_default"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_start_barrier"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="?attr/numberPogStyle"/>
            <ViewStub
                android:id="@+id/row_pog"
                android:layout="@layout/include_rds_pog"
                android:inflatedId="@+id/row_pog"
                android:layout_marginEnd="@dimen/rds_spacing_default"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_start_barrier"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="?attr/pogStyle"/>
            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/row_start_barrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierAllowsGoneWidgets="false"
                app:barrierDirection="end"
                app:constraint_referenced_ids="row_leading_icon, row_number_pog, row_pog"/>
            <com.robinhood.android.designsystem.row.component.RdsRowTextContainerView
                android:id="@+id/row_text_container"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_metadata_container"
                app:layout_constraintStart_toEndOf="@+id/row_start_barrier"
                app:layout_constraintTop_toTopOf="parent"
                app:primaryTextIconTint="?attr/colorForeground1"
                style="?attr/rowTextContainerStyle"/>
            <ViewStub
                android:id="@+id/row_metadata_container"
                android:layout="@layout/include_rds_row_metadata_container"
                android:inflatedId="@+id/row_metadata_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/rds_spacing_default"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_cta_text"
                app:layout_constraintStart_toEndOf="@+id/row_text_container"
                app:layout_constraintTop_toTopOf="parent"
                style="?attr/rowMetadataContainerStyle"/>
            <com.robinhood.android.designsystem.text.RhTextView
                android:id="@+id/row_cta_text"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/rds_spacing_default"
                android:layout_marginEnd="@dimen/rds_spacing_small"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_trailing_icon"
                app:layout_constraintStart_toEndOf="@+id/row_metadata_container"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_goneMarginEnd="0dp"
                style="?attr/rowCtaTextStyle"/>
            <ViewStub
                android:id="@+id/row_trailing_icon"
                android:visibility="gone"
                android:layout="@layout/include_rds_row_trailing_icon"
                android:inflatedId="@+id/row_trailing_icon"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toStartOf="@+id/row_caret_space"
                app:layout_constraintStart_toEndOf="@+id/row_cta_text"
                app:layout_constraintTop_toTopOf="parent"
                style="?attr/rowTrailingIconStyle"/>
            <Space
                android:id="@+id/row_caret_space"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginEnd="@dimen/rds_spacing_small"
                app:layout_constraintBottom_toBottomOf="@+id/row_caret"
                app:layout_constraintEnd_toStartOf="@+id/row_caret"
                app:layout_constraintTop_toTopOf="@+id/row_caret"
                app:layout_goneMarginEnd="0dp"/>
            <ViewStub
                android:id="@+id/row_caret"
                android:visibility="gone"
                android:layout="@layout/include_rds_row_caret"
                android:inflatedId="@+id/row_caret"
                app:layout_constraintBottom_toTopOf="@+id/row_bottom_barrier"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.5"
                style="?attr/rowCaretStyle"/>
            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/row_bottom_barrier"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:barrierDirection="bottom"
                app:constraint_referenced_ids="row_caret,row_caret_space,row_trailing_icon,row_cta_text,row_metadata_container,row_text_container,row_start_barrier,row_pog,row_number_pog,row_leading_icon"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <include
        android:layout_gravity="bottom"
        android:id="@+id/row_bottom_divider"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        layout="@layout/include_rds_divider"/>
</merge>
