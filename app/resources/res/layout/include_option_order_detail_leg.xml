<?xml version="1.0" encoding="utf-8"?>
<com.robinhood.android.options.history.detail.order.OptionOrderDetailLegView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceDisplaySmall"
        android:id="@+id/title_txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_xlarge"
        android:layout_marginBottom="@dimen/rds_spacing_small"
        android:layout_marginStart="@dimen/rds_row_horizontal_spacing"
        android:layout_marginEnd="@dimen/rds_row_horizontal_spacing"/>
    <com.robinhood.android.designsystem.row.RdsDataRowView
        android:id="@+id/option_order_detail_leg_side"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/option_order_detail_side"
        style="?attr/dataRowContainerStyle"/>
    <com.robinhood.android.designsystem.row.RdsDataRowView
        android:id="@+id/option_order_detail_leg_effect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/option_order_detail_effect"
        style="?attr/dataRowContainerStyle"/>
    <com.robinhood.android.designsystem.row.RdsDataRowView
        android:id="@+id/option_order_detail_leg_filled"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/option_order_detail_filled_date"
        style="?attr/dataRowContainerStyle"/>
    <com.robinhood.android.designsystem.row.RdsDataRowView
        android:id="@+id/option_order_detail_leg_filled_quantity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:labelText="@string/option_order_detail_filled_quantity"
        style="?attr/dataRowContainerStyle"/>
</com.robinhood.android.options.history.detail.order.OptionOrderDetailLegView>
