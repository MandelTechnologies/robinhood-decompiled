<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:id="@+id/divider_top"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:backgroundTint="?attr/colorForeground2"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/marquee_lottie_view"
        android:layout_width="wrap_content"
        android:layout_height="37sp"
        android:scaleType="centerCrop"
        android:adjustViewBounds="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_top"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/lottie_appearance_preferences_visualization_marquee"
        app:lottie_repeatCount="-1"
        app:lottie_repeatMode="restart"/>
    <View
        android:id="@+id/marquee_fade_start"
        android:background="@drawable/appearance_visualization_fade_start"
        android:layout_width="@dimen/rds_spacing_default"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/marquee_lottie_view"
        app:layout_constraintStart_toStartOf="@+id/marquee_lottie_view"
        app:layout_constraintTop_toTopOf="@+id/marquee_lottie_view"
        app:scarlet_affectedAttributes="background"/>
    <View
        android:id="@+id/marquee_fade_end"
        android:background="@drawable/appearance_visualization_fade_end"
        android:layout_width="@dimen/rds_spacing_default"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/marquee_lottie_view"
        app:layout_constraintEnd_toEndOf="@+id/marquee_lottie_view"
        app:layout_constraintTop_toTopOf="@+id/marquee_lottie_view"
        app:scarlet_affectedAttributes="background"/>
    <View
        android:id="@+id/divider_marquee"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:backgroundTint="?attr/colorForeground2"
        app:layout_constraintTop_toBottomOf="@+id/marquee_lottie_view"/>
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/chart_lottie_view"
        android:layout_width="wrap_content"
        android:layout_height="123sp"
        android:scaleType="fitCenter"
        android:adjustViewBounds="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider_marquee"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/lottie_appearance_preferences_visualization_chart"
        app:lottie_repeatCount="-1"
        app:lottie_repeatMode="restart"/>
    <View
        android:id="@+id/chart_fade_start"
        android:background="@drawable/appearance_visualization_fade_start"
        android:layout_width="@dimen/rds_spacing_default"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/chart_lottie_view"
        app:layout_constraintStart_toStartOf="@+id/chart_lottie_view"
        app:layout_constraintTop_toTopOf="@+id/chart_lottie_view"
        app:scarlet_affectedAttributes="background"/>
    <View
        android:id="@+id/divider_chart"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:backgroundTint="?attr/colorForeground2"
        app:layout_constraintTop_toBottomOf="@+id/chart_lottie_view"/>
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/sphere_lottie_view"
        android:layout_width="0dp"
        android:layout_height="100sp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/divider_sphere"
        app:layout_constraintHorizontal_weight="0.3"
        app:layout_constraintStart_toStartOf="@+id/chart_lottie_view"
        app:layout_constraintTop_toBottomOf="@+id/divider_chart"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/lottie_appearance_preferences_visualization_sphere"
        app:lottie_repeatCount="-1"
        app:lottie_repeatMode="restart"/>
    <View
        android:id="@+id/divider_sphere"
        android:background="#fff"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:backgroundTint="?attr/colorForeground2"
        app:layout_constraintBottom_toBottomOf="@+id/sphere_lottie_view"
        app:layout_constraintEnd_toStartOf="@+id/capsule_lottie_view"
        app:layout_constraintStart_toEndOf="@+id/sphere_lottie_view"
        app:layout_constraintTop_toTopOf="@+id/sphere_lottie_view"/>
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/capsule_lottie_view"
        android:layout_width="0dp"
        android:layout_height="100sp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="@+id/chart_lottie_view"
        app:layout_constraintHorizontal_weight="0.7"
        app:layout_constraintStart_toEndOf="@+id/divider_sphere"
        app:layout_constraintTop_toBottomOf="@+id/divider_chart"
        app:lottie_autoPlay="true"
        app:lottie_rawRes="@raw/lottie_appearance_preferences_visualization_capsule"
        app:lottie_repeatCount="-1"
        app:lottie_repeatMode="restart"/>
    <View
        android:id="@+id/divider_visualization"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:backgroundTint="?attr/colorForeground2"
        app:layout_constraintTop_toBottomOf="@+id/divider_sphere"/>
</merge>
