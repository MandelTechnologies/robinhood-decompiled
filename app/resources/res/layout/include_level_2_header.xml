<?xml version="1.0" encoding="utf-8"?>
<com.robinhood.android.equitydetail.p123ui.leveltwo.Level2HeaderView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="8dp"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="14sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_bid_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:text="@string/level_2_best_bid"
        android:drawablePadding="@dimen/legacy_spacing_extra_small"
        android:drawableEnd="@drawable/ic_rds_question_16dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginStart="@dimen/rds_spacing_default"
        android:drawableTint="?attr/colorForeground1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.robinhood.android.common.view.AnimatedRhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="24sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_bid_price_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginStart="@dimen/rds_spacing_default"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/best_bid_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_bid_quantity_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginStart="@dimen/rds_spacing_default"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/best_bid_price_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="14sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_ask_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:text="@string/level_2_best_ask"
        android:drawablePadding="@dimen/legacy_spacing_extra_small"
        android:drawableEnd="@drawable/ic_rds_question_16dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginEnd="@dimen/rds_spacing_default"
        android:drawableTint="?attr/colorForeground1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.robinhood.android.common.view.AnimatedRhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="24sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_ask_price_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginEnd="@dimen/rds_spacing_default"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/best_ask_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/best_ask_quantity_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/capsule_sans_medium"
        android:layout_marginEnd="@dimen/rds_spacing_default"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/best_ask_price_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmallBold"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/bids_asks_label_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:text="@string/level_2_bids"
        android:layout_marginStart="@dimen/rds_spacing_default"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmallBold"
        android:textSize="13sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/price_label_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="@string/level_2_price"
        android:layout_marginStart="@dimen/rds_spacing_default"
        app:layout_constraintEnd_toStartOf="@+id/shares_label_txt"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintHorizontal_weight="3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bids_asks_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmallBold"
        android:textSize="13sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/shares_label_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/level_2_shares"
        app:layout_constraintEnd_toStartOf="@+id/total_value_label_txt"
        app:layout_constraintHorizontal_weight="3"
        app:layout_constraintStart_toEndOf="@+id/price_label_txt"
        app:layout_constraintTop_toTopOf="@+id/price_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmallBold"
        android:textSize="13sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/total_value_label_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/level_2_total_value"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_weight="4"
        app:layout_constraintStart_toEndOf="@+id/shares_label_txt"
        app:layout_constraintTop_toTopOf="@+id/shares_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/price_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/price_label_txt"
        app:layout_constraintStart_toStartOf="@+id/price_label_txt"
        app:layout_constraintTop_toBottomOf="@+id/price_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/shares_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/shares_label_txt"
        app:layout_constraintStart_toStartOf="@+id/shares_label_txt"
        app:layout_constraintTop_toBottomOf="@+id/shares_label_txt"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularMedium"
        android:textSize="18sp"
        android:textColor="?attr/colorForeground1"
        android:id="@+id/total_value_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/total_value_label_txt"
        app:layout_constraintStart_toStartOf="@+id/total_value_label_txt"
        app:layout_constraintTop_toBottomOf="@+id/total_value_label_txt"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/static_group"
        android:visibility="visible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="best_bid_label_txt,best_bid_price_txt,best_bid_quantity_txt,best_ask_label_txt,best_ask_price_txt,best_ask_quantity_txt"/>
    <androidx.constraintlayout.widget.Group
        android:id="@+id/scrub_group"
        android:visibility="invisible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="bids_asks_label_txt,price_label_txt,shares_label_txt,total_value_label_txt,price_txt,shares_txt,total_value_txt"/>
    <com.robinhood.android.common.view.RhCardView
        android:theme="?attr/surfaceTheme"
        android:id="@+id/graph_cardview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginStart="@dimen/rds_spacing_small"
        android:layout_marginEnd="@dimen/rds_spacing_small"
        android:backgroundTint="?attr/colorBackground1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/best_bid_quantity_txt">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.robinhood.android.equitydetail.p123ui.leveltwo.Level2Graph
                android:id="@+id/level_2_graph"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_marginTop="@dimen/rds_spacing_small"
                app:l2g_askColor="?attr/colorNegative"
                app:l2g_bidColor="?attr/colorPositive"
                app:l2g_bidFillDrawable="@drawable/level_2_graph_fill_tile"
                app:l2g_strokeWidth="2dp"/>
            <com.robinhood.android.equitydetail.p123ui.leveltwo.Level2AxesView
                android:id="@+id/level_2_axes"
                android:padding="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </com.robinhood.android.common.view.RhCardView>
</com.robinhood.android.equitydetail.p123ui.leveltwo.Level2HeaderView>
