<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.robinhood.android.common.p088ui.view.FragmentContainerLayout
        android:id="@+id/education_tour_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="@dimen/rds_elevation_tight"
        android:outlineProvider="none"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_tab"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.robinhood.android.common.p088ui.view.RhToolbar
            android:id="@+id/toolbar_wrapper"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:scarlet_childContext="animatedAndInherit"
            style="?android:attr/toolbarStyle"/>
        <com.robinhood.android.common.p088ui.view.FragmentContainerLayout
            android:id="@+id/tab_content_container"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/mobile_linking_collapsed"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:id="@+id/snackbar_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            app:layout_constraintBottom_toTopOf="@+id/mobile_linking_collapsed"/>
        <androidx.compose.p011ui.platform.ComposeView
            android:id="@+id/mobile_linking_collapsed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="?attr/colorTransparent"
            app:layout_constraintBottom_toTopOf="@+id/compose_tab_bar"/>
        <View
            android:id="@+id/bottom_nav_bar_divider"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:backgroundTint="?attr/colorBackground3"
            app:layout_constraintBottom_toTopOf="@+id/compose_tab_bar"/>
        <androidx.compose.p011ui.platform.ComposeView
            android:id="@+id/compose_tab_bar"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="?attr/colorBackground1"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
