<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/cardview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardBackgroundColor="?attr/colorBackground1"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/numpad"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/content"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </androidx.cardview.widget.CardView>
    <com.robinhood.android.designsystem.numpad.RdsNumpadView
        android:id="@+id/numpad"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="?attr/colorBackground1"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <FrameLayout
        android:id="@+id/cta_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/rds_spacing_medium"
        android:layout_marginEnd="@dimen/rds_spacing_medium"
        app:layout_constraintBottom_toTopOf="@+id/numpad">
        <com.robinhood.android.designsystem.button.RdsButton
            android:id="@+id/review_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/general_action_review"
            style="?attr/primaryButtonStyle"/>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/quick_trade_amounts_container"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.robinhood.android.designsystem.button.RdsButton
                android:ellipsize="end"
                android:id="@+id/quick_trade_amount_one_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_weight="1"
                style="?attr/secondaryButtonStyle"/>
            <com.robinhood.android.designsystem.button.RdsButton
                android:ellipsize="end"
                android:id="@+id/quick_trade_amount_two_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_weight="1"
                android:layout_marginStart="@dimen/rds_spacing_default"
                style="?attr/secondaryButtonStyle"/>
            <com.robinhood.android.designsystem.button.RdsButton
                android:ellipsize="end"
                android:id="@+id/quick_trade_amount_three_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_weight="1"
                android:layout_marginStart="@dimen/rds_spacing_default"
                style="?attr/secondaryButtonStyle"/>
            <com.robinhood.android.designsystem.button.RdsButton
                android:ellipsize="end"
                android:id="@+id/quick_trade_sell_all_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/order_create_sell_all"
                android:maxLines="1"
                android:layout_weight="1"
                android:layout_marginStart="@dimen/rds_spacing_default"
                style="?attr/secondaryButtonStyle"/>
        </LinearLayout>
    </FrameLayout>
    <com.robinhood.android.designsystem.text.RhTextView
        android:gravity="center"
        android:id="@+id/swipe_hint"
        android:padding="@dimen/rds_spacing_small"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/rds_spacing_default"
        android:text="@string/order_review_swipe_to_submit_prompt"
        android:drawableTop="0x7f080441"
        android:drawablePadding="@dimen/rds_spacing_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/SwipeHintText"/>
    <com.robinhood.android.trade.equity.p261ui.dialog.equitytooltip.EquityOrderTypeTooltipView
        android:id="@+id/order_type_onboarding_tooltip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="?attr/tooltipStyle"/>
    <com.robinhood.android.trade.equity.p261ui.dialog.MarketSessionChangeAlertView
        android:id="@+id/market_session_change_alert"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/snackbar_shim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/cta_container"/>
    <com.robinhood.android.common.p088ui.ShimmerLoadingView
        android:id="@+id/equity_dollar_order_loading_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
