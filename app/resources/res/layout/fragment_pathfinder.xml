<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:theme="@style/ThemeOverlay.Robinhood.DesignSystem.Achromatic"
    android:id="@+id/pathfinder_container"
    android:clipToPadding="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.robinhood.android.lib.pathfinder.views.PathfinderWebView
        android:id="@+id/pathfinder_webview"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.robinhood.android.lib.webview.view.WebLoadingView
        android:id="@+id/pathfinder_web_loading_view"
        android:background="#fff"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/contact_web_loading"
        android:backgroundTint="?attr/colorBackground1"/>
    <com.robinhood.android.lib.webview.view.WebErrorView
        android:id="@+id/pathfinder_web_error_view"
        android:background="#fff"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="?attr/colorBackground1"/>
    <FrameLayout
        android:id="@+id/new_webview_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.robinhood.android.designsystem.progress.RdsProgressBar
        android:layout_gravity="top"
        android:id="@+id/loading_progress_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/ProgressBar"/>
</FrameLayout>
