<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.robinhood.android.designsystem.numpad.RdsNumpadView
        android:id="@+id/numpad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_for_fab"
        style="?attr/numpadStyle"/>
    <com.robinhood.android.common.view.RhFloatingActionButton
        android:layout_gravity="end|top"
        android:id="@+id/fab_btn"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_for_fab"
        android:contentDescription="@string/general_label_done"
        app:pullUp="true"
        app:srcCompat="@drawable/svg_ic_check_24dp"
        app:tint="?attr/colorBackground1"
        style="@style/FloatingActionButton"/>
</merge>
