<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/cardview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:cardBackgroundColor="?attr/colorBackground1"
        app:cardElevation="0dp"
        app:layout_constraintBottom_toTopOf="@+id/design_system_review_container"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <com.robinhood.utils.p409ui.view.BlockableNestedScrollView
            android:id="@+id/content_scrollview"
            android:scrollbars="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/content"
                android:padding="@dimen/rds_spacing_default"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </com.robinhood.utils.p409ui.view.BlockableNestedScrollView>
    </androidx.cardview.widget.CardView>
    <com.robinhood.android.designsystem.numpad.RdsNumpadView
        android:id="@+id/numpad"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="?attr/colorBackground1"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/design_system_review_container"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="?attr/colorBackground1"
        app:layout_constraintBottom_toTopOf="@+id/numpad">
        <com.robinhood.android.designsystem.button.RdsTextButton
            android:layout_gravity="center_horizontal"
            android:id="@+id/sell_all_btn"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/rds_spacing_small"
            android:layout_marginBottom="@dimen/rds_spacing_small"
            android:layout_marginVertical="@dimen/rds_spacing_small"
            style="?attr/textButtonStyle"/>
        <com.robinhood.android.lib.trade.view.TradeAccountSwitcherView
            android:layout_gravity="center_horizontal"
            android:id="@+id/account_switcher_btn"
            android:paddingBottom="@dimen/rds_spacing_default"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.robinhood.android.designsystem.button.RdsButton
            android:id="@+id/review_btn"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/general_action_review"
            android:layout_marginStart="@dimen/rds_spacing_medium"
            android:layout_marginEnd="@dimen/rds_spacing_medium"
            style="?attr/primaryButtonStyle"/>
        <androidx.compose.p011ui.platform.ComposeView
            android:id="@+id/twenty_four_hour_market_review_button_container"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <Space
            android:id="@+id/numpad_top_space_replacement"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/numpad_top_space_height"/>
    </LinearLayout>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/snackbar_shim"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:elevation="2dp"
        app:layout_constraintBottom_toTopOf="@+id/design_system_review_container"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:gravity="center"
        android:id="@+id/swipe_hint"
        android:padding="@dimen/rds_spacing_small"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/rds_spacing_default"
        android:text="@string/order_review_swipe_to_submit_prompt"
        android:drawableTop="0x7f080441"
        android:drawablePadding="@dimen/rds_spacing_small"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/SwipeHintText"/>
    <com.robinhood.android.trade.equity.p261ui.dialog.equitytooltip.EquityOrderTypeTooltipView
        android:id="@+id/order_type_onboarding_tooltip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="?attr/tooltipStyle"/>
    <com.robinhood.android.trade.equity.p261ui.dialog.MarketSessionChangeAlertView
        android:id="@+id/market_session_change_alert"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.robinhood.android.common.p088ui.ShimmerLoadingView
        android:id="@+id/equity_order_loading_view"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
