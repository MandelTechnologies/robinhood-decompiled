<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_title_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_min="200dp"
        style="@style/OrderTextNewDesign.Title"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_description_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:layout_marginTop="@dimen/order_header_internal_margin"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_title_txt"
        app:layout_constraintWidth_min="100dp"
        style="@style/OrderTextNewDesign.Subtitle"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:enabled="false"
        android:id="@+id/direct_ipo_shares_quantity_label_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:layout_marginTop="@dimen/order_header_bottom_margin"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_description_txt"
        app:layout_constraintWidth_min="140dp"
        style="@style/OrderTextNewDesign.Label"/>
    <com.robinhood.android.common.p088ui.view.RhEditText
        android:textColor="?android:attr/textColorPrimaryNoDisable"
        android:id="@+id/direct_ipo_shares_quantity_edt"
        android:maxLength="12"
        android:inputType="number"
        app:layout_constraintBaseline_toBaselineOf="@+id/direct_ipo_shares_quantity_label_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/direct_ipo_shares_quantity_label_txt"
        style="@style/OrderTextNewDesign.InputField">
        <requestFocus/>
    </com.robinhood.android.common.p088ui.view.RhEditText>
    <include
        android:id="@+id/direct_ipo_shares_quantity_edt_bottom_divider"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/rds_spacing_default"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_shares_quantity_label_txt"
        layout="@layout/include_rds_divider"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_price_label_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:paddingBottom="0dp"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:drawablePadding="@dimen/rds_spacing_small"
        android:drawableEnd="@drawable/ic_rds_question_16dp"
        android:backgroundTint="?attr/colorTransparent"
        android:drawableTint="?attr/colorPrimary"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_shares_quantity_edt_bottom_divider"
        app:layout_constraintWidth_min="120dp"
        style="@style/OrderTextNewDesign.Label"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_price_subtitle_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:visibility="gone"
        android:layout_marginTop="@dimen/rds_spacing_xxsmall"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_price_label_txt"
        app:layout_constraintWidth_min="150dp"
        style="@style/OrderTextNewDesign.Subtitle"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_set_price_type_btn"
        android:visibility="gone"
        android:layout_marginTop="@dimen/rds_spacing_xxsmall"
        android:drawableEnd="@null"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_price_subtitle_txt"
        style="@style/OrderTextNewDesign.Subtitle.Entry"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_price_value_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintBottom_toTopOf="@+id/direct_ipo_price_bottom_divider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/direct_ipo_price_label_txt"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_shares_quantity_edt_bottom_divider"
        app:layout_constraintWidth_min="80dp"
        style="@style/OrderTextNewDesign.Field"/>
    <com.robinhood.android.common.p088ui.view.RhMoneyInputView
        android:gravity="end"
        android:id="@+id/direct_ipo_custom_price_edt"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:amountMaxLength="12"
        app:amountTextAppearance="@style/OrderTextNewDesign.CurrencyField"
        app:currencyTextAppearance="?attr/textAppearanceRegularMediumBold"
        app:layout_constraintBottom_toTopOf="@+id/direct_ipo_price_bottom_divider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/direct_ipo_price_label_txt"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_shares_quantity_edt_bottom_divider"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/direct_ipo_price_barrier"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="direct_ipo_price_subtitle_txt,direct_ipo_price_value_txt,direct_ipo_price_label_txt,direct_ipo_custom_price_edt,direct_ipo_set_price_type_btn"/>
    <include
        android:id="@+id/direct_ipo_price_bottom_divider"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/rds_spacing_default"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_price_barrier"
        layout="@layout/include_rds_divider"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_total_label_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:layout_marginTop="@dimen/rds_spacing_default"
        android:drawablePadding="@dimen/rds_spacing_small"
        android:backgroundTint="?attr/colorTransparent"
        android:drawableTint="?attr/colorPrimary"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_price_bottom_divider"
        app:layout_constraintWidth_min="100dp"
        style="@style/OrderTextNewDesign.Label"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:id="@+id/direct_ipo_total_value_txt"
        android:background="@drawable/order_text_placeholder_background"
        android:backgroundTint="?attr/colorTransparent"
        app:layout_constraintBaseline_toBaselineOf="@+id/direct_ipo_total_label_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/direct_ipo_total_label_txt"
        app:layout_constraintWidth_min="110dp"
        style="@style/OrderTextNewDesign.Field"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmall"
        android:id="@+id/direct_ipo_order_disclaimer_txt"
        android:visibility="visible"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/rds_spacing_large"
        android:drawablePadding="@dimen/rds_spacing_small"
        android:drawableTint="?android:attr/textColorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_total_label_txt"
        app:layout_constraintVertical_bias="1"
        app:useLinkMovementMethod="true"/>
    <com.robinhood.android.designsystem.text.RhTextView
        android:textAppearance="?attr/textAppearanceRegularSmall"
        android:gravity="bottom"
        android:id="@+id/direct_ipo_review_txt"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/rds_spacing_large"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/direct_ipo_total_label_txt"
        app:useLinkMovementMethod="true"/>
</merge>
